<%= error_messages_for :preview %>

<% form_for(:preview, :url => previews_path, 
                      :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="preview">Upload A Preview:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag 'Create' %>
  </p>
<% end -%>